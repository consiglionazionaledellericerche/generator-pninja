<?php

namespace App\Models;
use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\Factories\HasFactory;

use Laravel\Scout\Searchable;

class AcRule extends Model
{
    use HasFactory;
    use Searchable;

    protected $table = 'ac_rules';
    protected $fillable = [
        'ptype',
        'v0',
        'v1',
        'v2',
        'v3',
        'v4',
        'v5'
    ];
    public function toSearchableArray()
    {
        return [
            'id' => $this->id,
            'ptype' => $this->ptype,
            'v0' => $this->v0,
            'v1' => $this->v1,
            'v2' => $this->v2,
            'v3' => $this->v3,
            'v4' => $this->v4,
            'v5' => $this->v5,
        ];
    }
}