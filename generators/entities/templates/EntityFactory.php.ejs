    protected function getRandomUniqueValue(array $idsA, array $idsB) {
        $uniqueValues = array_diff($idsA, $idsB);
        if (!empty($uniqueValues)) {
            return $uniqueValues[array_rand($uniqueValues)];
        }
        return null;
    }

