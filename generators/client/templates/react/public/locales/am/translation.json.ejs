  "entities": {
    "entities": "አካላት",
    <%- entities.map(entity => `"${to.snake(entity.name)}": {${[
      `\n      "${entity.name}": "${to.capital(entity.name)}"`,
      `"${pluralize(entity.name)}": "${to.capital(pluralize(entity.name))}"`,
      `"id": "ID"`,
      ...(entity.body.map(r => `"${to.snake(r.name)}": "${to.capital(r.name)}"`)),
      ...(getModelForeignIds(entity, relationships).map(r => `"${to.snake(r.foreignId)}": "${to.capital(r.foreignId)}"`)),
      ...(getModelRelatedEntities(entity, relationships).map(r => `"${to.snake(r.field)}": "${to.capital(r.field)}"`))
    ].join(`,\n      `)}\n    }`).join(`,\n    `)%>
  },
