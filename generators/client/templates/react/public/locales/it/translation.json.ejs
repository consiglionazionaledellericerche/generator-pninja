{
  "appName": "<%= appName %>",
    "dbms": {
    "sqlite": "SQLite",
    "mysql": "MySQL",
    "mariadb": "MariaDB",
    "pgsql": "PostgreSQL",
    "sqlsrv": "SQL Server"
  },
  "entities": {
    <%- entities.map(entity => `"${to.snake(entity.name)}": {${[
      `\n      "${to.capital(entity.name)}": "${to.capital(entity.name)}"`,
      `\n      "${to.capital(pluralize(entity.name))}": "${to.capital(pluralize(entity.name))}"`,
      `"id": "ID"`,
      ...(entity.body.map(r => `"${to.snake(r.name)}": "${to.capital(r.name)}"`)),
      ...(getModelForeignIds(entity, relationships).map(r => `"${to.snake(r.foreignId)}": "${to.capital(r.foreignId)}"`)),
      ...(getModelRelatedEntities(entity, relationships).map(r => `"${to.snake(r.field)}": "${to.capital(r.field)}"`))
    ].join(`,\n      `)}\n    }`).join(`,\n    `)%>
  },
  "errors": {
    "401": {
      "title": "Errore 401 – Accesso non autorizzato",
      "message": "Devi accedere per visualizzare questa pagina. Controlla le tue credenziali e riprova."
    },
    "403": {
      "title": "Ops! Errore 403 – Mi dispiace, {{username}}",
      "message": "Ciao <1>{{username}}</1>, ti abbiamo riconosciuto, ma non hai i permessi necessari per accedere a questa pagina. Se c’è un errore, contattaci!"
    },
    "404": {
      "title": "Ops! Errore 404 – Pagina non trovata",
      "message": "Sembra che la pagina che stai cercando non esista più. Potrebbe essere stata spostata o eliminata. Controlla l'URL o torna alla homepage!"
    }
  },
  "pages": {
    "Home": "Home"
  },
  "ui": {
    "account": {
      "login": "Login",
      "logout": "Logout"
    },
    "darkmode": {
      "light": "Passa alla modalità chiara",
      "dark": "Passa alla modalità scura"
    }
  }
}