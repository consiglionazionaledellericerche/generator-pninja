{
  "appName": "<%= appName %>",
  "actions": {
    "crud": {
      "create": "Créer",
      "read": "Lire",
      "update": "Mettre à jour",
      "delete": "Supprimer"
    }
  },
  "common": {
    "select": {
      "placeholder": "Sélectionnez une option",
      "noOptions": "Aucune option",
      "loading": "Chargement des options..."
    },
    "show": {
      "more": "Afficher plus",
      "less": "Afficher moins"
    }
  },
  "dbms": {
    "sqlite": "SQLite",
    "mysql": "MySQL",
    "mariadb": "MariaDB",
    "pgsql": "PostgreSQL",
    "sqlsrv": "SQL Server"
  },
  "entities": {
    <%- entities.map(entity => `"${to.snake(entity.name)}": {${[
      `\n      "${entity.name}": "${to.capital(entity.name)}"`,
      `"${pluralize(entity.name)}": "${to.capital(pluralize(entity.name))}"`,
      `"id": "ID"`,
      ...(entity.body.map(r => `"${to.snake(r.name)}": "${to.capital(r.name)}"`)),
      ...(getModelForeignIds(entity, relationships).map(r => `"${to.snake(r.foreignId)}": "${to.capital(r.foreignId)}"`)),
      ...(getModelRelatedEntities(entity, relationships).map(r => `"${to.snake(r.field)}": "${to.capital(r.field)}"`))
    ].join(`,\n      `)}\n    }`).join(`,\n    `)%>
  },
  "errors": {
    "401": {
      "title": "Erreur 401 – Accès non autorisé",
      "message": "Vous devez vous connecter pour accéder à cette page. Vérifiez vos identifiants et réessayez."
    },
    "403": {
      "title": "Oups ! Erreur 403 – Désolé, {{username}}",
      "message": "Salut <1>{{username}}</1>, on te reconnaît, mais tu n’as pas les permissions nécessaires pour accéder à cette page. Si c’est une erreur, fais-nous signe !"
    },
    "404": {
      "title": "Oups ! Erreur 404 – Page introuvable",
      "message": "On dirait que la page que tu cherches n’existe plus. Elle a peut-être été déplacée ou supprimée. Vérifie l’URL ou retourne à la page d’accueil !"
    },
    "entities": {
      "create": "Erreur lors de la création",
      "read": "Erreur lors de la lecture",
      "update": "Erreur lors de la mise à jour",
      "delete": "Erreur lors de la suppression"
    },
    "options": {
      "load": "Erreur de chargement des options pour"
    }
  },
  "pages": {
    "Home": "Accueil"
  },
  "ui": {
    "account": {
      "login": "Login",
      "logout": "Logout"
    },
    "darkmode": {
      "light": "Passer en mode clair",
      "dark": "Passer en mode sombre"
    }
  }
}