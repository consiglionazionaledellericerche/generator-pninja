{
  "appName": "<%= appName %>",
  "actions": {
    "crud": {
      "back": "Back",
      "cancel": "Cancel",
      "create": "Create",
      "edit": "Edit",
      "delete": "Delete",
      "save": "Save",
      "update": "Update",
      "view": "View"
    }
  },
  "common": {
    "select": {
      "placeholder": "Select an option",
      "noOptions": "No options",
      "loading": "Loading options..."
    },
    "show": {
      "more": "Show more",
      "less": "Show less"
    }
  },
  "dbms": {
    "sqlite": "SQLite",
    "mysql": "MySQL",
    "mariadb": "MariaDB",
    "pgsql": "PostgreSQL",
    "sqlsrv": "SQL Server"
  },
  "entities": {
    <%- entities.map(entity => `"${to.snake(entity.name)}": {${[
      `\n      "${entity.name}": "${to.capital(entity.name)}"`,
      `"${pluralize(entity.name)}": "${to.capital(pluralize(entity.name))}"`,
      `"id": "ID"`,
      ...(entity.body.map(r => `"${to.snake(r.name)}": "${to.capital(r.name)}"`)),
      ...(getModelForeignIds(entity, relationships).map(r => `"${to.snake(r.foreignId)}": "${to.capital(r.foreignId)}"`)),
      ...(getModelRelatedEntities(entity, relationships).map(r => `"${to.snake(r.field)}": "${to.capital(r.field)}"`))
    ].join(`,\n      `)}\n    }`).join(`,\n    `)%>
  },
  "errors": {
    "401": {
      "title": "Error 401 – Unauthorized",
      "message": "You need to log in to access this page. Please check your credentials and try again."
    },
    "403": {
      "title": "Oops! Error 403 – Sorry, {{username}}",
      "message": "Hey <1>{{username}}</1>, we recognize you, but you don’t have the right permissions to access this page. If you think this is a mistake, reach out to us!"
    },
    "404": {
      "title": "Oops! Error 404 – Page Not Found",
      "message": "Looks like the page you're looking for doesn’t exist anymore. It might have been moved or deleted. Check the URL or head back to the homepage!"
    },
    "entities": {
      "create": "Error creating",
      "read": "Error reading",
      "update": "Error updating",
      "delete": "Error deleting"
    },
    "options": {
      "load": "Error loading options for"
    },
    "validations": {
      "file": {
        "size": "The file is too large. The maximum allowed size is {{maxSize}}.",
        "type": "The file type is not supported"
      }
    }
  },
  "pages": {
    "Home": "Dashboard"
  },
  "ui": {
    "account": {
      "login": "Login",
      "loggingIn": "Logging in...",
      "logout": "Logout",
      "loggingOut": "Logging out...",
      "logoutError": "Error logging out",
      "unknownError": "Unknown error"
    },
    "darkmode": {
      "light": "Switch to light mode",
      "dark": "Switch to dark mode"
    }
  }
}