{
  "App Name": "<%= appName %>",
  "menu": {
    "entities": "Entities",
    "language": "Language",
    "home": "Home"
  },
  "actions": {
    "entity": {
      "create": "Create",
      "save": "Save",
      "view": "View",
      "edit": "Edit",
      "delete": "Delete",
      "deleteConfirm": "Confirm delete"
    }
  },
  "entities": {<% entities.forEach(function(entity, index) { %>
    "<%= entity.name %>": "<%= to.sentence(entity.name) %>",
    "<%= pluralize(entity.name) %>": "<%= pluralize(to.sentence(entity.name)) %>",
    "<%= entity.variable %>": {
      <% entity._presto__properties.forEach(function(property, jndex) { %>"<%= property.column%>": "<%= to.sentence(property.column)%>",
      <% }); %>"id": "ID"
    }<% if(index < entities.length - 1) {%>,<% }%><% }); %>
  },
  "warnings": {
    "recordNotFound": "Record does not exists"
  }
}
